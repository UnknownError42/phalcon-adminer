{
  "name": "phalcon/adminer",
  "description": "Phalcon Adminer",
  "keywords": [
    "adminer",
    "phalcon",
    "framework",
    "extension"
  ],
  "type": "project",
  "autoload": {
    "psr-4": {
      "App\\": "app/classes/"
    }
  },
  "require": {
    "php" : "^7.1.3",
    "ext-curl": "*",
    "ext-mbstring": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "vlucas/phpdotenv": "^2.4",
    "guzzlehttp/guzzle": "~6.0",
    "swiftmailer/swiftmailer": "^5.4",
    "dariuszp/cli-progress-bar": "^1.0"
  },
  "require-dev": {
    "phalcon/devtools": "v3.4",
    "phalcon/incubator": "^3.4",
    "phpunit/phpunit": "^4.8",
    "codeception/codeception": "^2.2",
    "codeception/verify": "~0.3",
    "codeception/specify": "~0.4",
    "codeception/mockery-module": "^0.2",
    "squizlabs/php_codesniffer": "~2.7",
    "snowair/phalcon-debugbar": "^1.2"
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true
  }
}
